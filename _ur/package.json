{
  "name": "@ursys/core",
  "version": "0.1",
  "description": "modular system for enhancing NetCreate 2.0 (ITEST)",
  "repository": {
    "type": "git",
    "url": "https://github.com/netcreateorg/netcreate-itest"
  },
  "private": true,
  "scripts": {
    "build-core": "node ./npm-scripts/@build-core.mts",
    "_types-node": "npx tsc --project ./npm-scripts/tsconfig-types-node.json && npx dts-bundle-generator --config ./npm-scripts/types-bundle-node.json",
    "_types-browser": "npx tsc --project ./npm-scripts/tsconfig-types-browser.json && npx dts-bundle-generator --config ./npm-scripts/types-bundle-browser.json",
    "build-types": "npm run _types-node && npm run _types-browser ",
    "lint": "eslint .",
    "clean": "rm -rf ./node_modules ./_dist"
  },
  "browser": "./_dist/client-cjs.js",
  "main": "./_dist/server.cjs",
  "exports": {
    "node": {
      "import": "./_dist/server-esm.mjs",
      "require": "./_dist/server.cjs"
    },
    "browser": {
      "import": "./_dist/client-esm.js",
      "require": "./_dist/client-cjs.js",
      "default": "./_dist/client-umd.js"
    }
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.21.4",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-umd-wrapper": "^2.0.0",
    "ts-node": "^10.9.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "keyv": "^4.5.4",
    "keyv-file": "^0.2.0",
    "lokijs": "^1.5.12",
    "node-dir": "^0.1.17",
    "ws": "^8.14.2"
  }
}
