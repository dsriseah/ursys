{
  "name": "@ursys/core",
  "version": "0.1",
  "description": "support system for realtime web app prototyping",
  "private": true,
  "scripts": {
    "build-libs": "node ./npm-scripts/@build-lib.cjs",
    "lint": "eslint .",
    "clean": "rm -rf ./node_modules ./_dist"
  },
  "browser": "./_dist/client-cjs.js",
  "main": "./_dist/server.cjs",
  "exports": {
    "node": {
      "import": "./_dist/server-esm.mjs",
      "require": "./_dist/server.cjs"
    },
    "browser": {
      "import": "./_dist/client-esm.js",
      "require": "./_dist/client-cjs.js",
      "default": "./_dist/client-umd.js"
    }
  },
  "devDependencies": {
    "esbuild": "^0.19.2",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-umd-wrapper": "^1.0.7"
  },
  "dependencies": {
    "@achrinza/node-ipc": "^10.1.10",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "keyv": "^4.5.4",
    "keyv-file": "^0.2.0",
    "lokijs": "^1.5.12",
    "node-dir": "^0.1.17",
    "ts-node": "^10.9.1",
    "ws": "^8.14.2"
  }
}
