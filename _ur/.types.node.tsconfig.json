{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "allowImportingTsExtensions": true,
    "typeRoots": ["./_ur/_types/"],
    "paths": {
      "*": ["./_ur/_types/*"]
    },
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./_ur/_dist/types",
    "module": "NodeNext",
    "target": "ESNext"
  },
  "include": ["./_ur/node-server/**/*.mts", "./_ur/common/**/*", "./_ur/_types/**/*"],
  "exclude": [
    "./_ur/web-client",
    "./_ur/node_modules",
    "./_ur/_dist",
    "./_ur/npm-scripts",
    "./_ur/test-config",
    "./_ur/test",
    "./_ur/vendor"
  ]
}
